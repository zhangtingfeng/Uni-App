{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "packages/applications",
  "projects": {
    "starterkitdashboard": {
      "projectType": "library",
      "root": "packages/modules/starterkitdashboard",
      "sourceRoot": "packages/modules/starterkitdashboard/src",
      "prefix": "starterkit",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "packages/modules/starterkitdashboard/tsconfig.lib.json",
            "project": "packages/modules/starterkitdashboard/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/modules/starterkitdashboard/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/modules/starterkitdashboard/src/test.ts",
            "tsConfig": "packages/modules/starterkitdashboard/tsconfig.spec.json",
            "karmaConfig": "packages/modules/starterkitdashboard/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "packages/modules/starterkitdashboard/tsconfig.lib.json",
              "packages/modules/starterkitdashboard/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "starterkit": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "packages/applications/starterkit",
      "sourceRoot": "packages/applications/starterkit/src",
      "prefix": "starterkit",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/starterkit",
            "index": "packages/applications/starterkit/src/index.html",
            "main": "packages/applications/starterkit/src/main.ts",
            "polyfills": "packages/applications/starterkit/src/polyfills.ts",
            "tsConfig": "packages/applications/starterkit/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "packages/applications/starterkit/src/favicon.ico",
              "packages/applications/starterkit/src/assets",
              "packages/applications/starterkit/src/manifest.webmanifest"
            ],
            "styles": [
              "packages/applications/starterkit/src/styles.scss"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "lodash",
              "buffer",
              "uild",
              "uuid",
              "camelcase-keys",
              "@aws-crypto/sha256-js",
              "ulid",
              "@aws-crypto/sha256-browser",
              "fast-xml-parser",
              "@aws-crypto/crc32",
              "crypto-js/core",
              "crypto-js/hmac-sha256",
              "crypto-js/lib-typedarrays",
              "isomorphic-unfetch",
              "sockjs-client",
              "aggrid-common"
           ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "packages/applications/starterkit/src/environments/environment.ts",
                  "with": "packages/applications/starterkit/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "starterkit:build:production"
            },
            "development": {
              "browserTarget": "starterkit:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "starterkit:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/applications/starterkit/src/test.ts",
            "polyfills": "packages/applications/starterkit/src/polyfills.ts",
            "tsConfig": "packages/applications/starterkit/tsconfig.spec.json",
            "karmaConfig": "packages/applications/starterkit/karma.conf.js",
	    "inlineStyleLanguage": "scss",
            "assets": [
              "packages/applications/starterkit/src/favicon.ico",
              "packages/applications/starterkit/src/assets",
              "packages/applications/starterkit/src/manifest.webmanifest"
            ],
            "styles": [
              "packages/applications/starterkit/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "packages/applications/starterkit/tsconfig.app.json",
              "packages/applications/starterkit/tsconfig.spec.json",
              "packages/applications/starterkit/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "packages/applications/starterkit/e2e/protractor.conf.js",
            "devServerTarget": "starterkit:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "starterkit:serve:production"
            }
          }
        }
      }
    }},
  "cli": {
    "analytics": "7b20564d-519e-4b71-9fd8-1f851c328aab",
    "packageManager": "yarn"
  },
  "defaultProject": "starterkitdashboard"
}